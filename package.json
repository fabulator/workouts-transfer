  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "develop": "tsc --watch",
    "lint": "eslint ./ --ext .js --ext .json --ext .ts",
    "lint:export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build && npm run test:build",
    "release": "semantic-release",
    "test": "jest",
    "test:all": "npm run lint && npm run tsc && npm run test",
    "test:build": "npm run test -- --config jest.config.build.js",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@types/luxon": "^1.27.1",
    "cross-fetch": "^3.1.4",
    "inversify": "^5.1.1",
    "luxon": "^2.0.1",
    "mathjs": "^9.4.4",
    "reflect-metadata": "^0.1.13",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "commitlint-config-fabulator": "^2.0.2",
    "eslint-config-fabulator": "^1.2.3",
    "fitbit-api-handler": "^7.0.0",
    "fitness-libraries": "^5.11.0",
    "fitness-models": "^4.1.1",
    "garmin-api-handler": "^5.0.5",
    "jest-config-fabulator": "^1.1.0",
    "semantic-release-config-fabulator": "^2.6.2",
    "strava-api-handler": "^5.0.2",
    "ts-jest": "^27.0.3"
  },
  "peerDependencies": {
    "fitbit-api-handler": "^7.0.0",
